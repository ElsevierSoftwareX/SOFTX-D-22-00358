<!DOCTYPE html>
<html lang="en">
<head>
	<title>Search with Twitter</title>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<link rel="icon" type="image/png" href="static/Login_v12/images/icons/hash.ico"/>

	<link href="{{url_for('static',filename='Login_v12/vendor/designing/css/designing.min.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='Login_v12/fonts/font-awesome-4.7.0/css/font-awesome.min.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='Login_v12/fonts/Linearicons-Free-v1.0.0/icon-font.min.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='vendor/animate/animate.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='vendor/css-hamburgers/hamburgers.min.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='vendor/select2/select2.min.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='css/util.css')}}" rel="stylesheet" type="text/css">
  	<link href="{{url_for('static',filename='css/main.css')}}" rel="stylesheet" type="text/css">

  <!-- Template Main CSS File -->
  <link href="{{url_for('static',filename='css/style.css')}}" rel="stylesheet" type="text/css">
	
<body>

	<div class="limiter">
		<div class="container-login100" style="background-image: url('static/images/tweet.jpeg');">
			<h1>Search fake posts from Twitter</h1>
			<div class="wrap-login100 p-t-190 p-b-30">
				<form class="login100-form validate-form" action="{{ url_for('twitteroutput')}}" method="post">
					<div class="login100-form-avatar">
						<img src="static/images/chiraiya2.png" alt="AVATAR">
					</div>

					<span class="login100-form-title p-t-20 p-b-45">
						
					</span>

					<div class="wrap-input100 validate-input m-b-10" data-validate = "Hashtag is required">
						<input class="input100" type="text" name="HashValue" placeholder="Search hashtag">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-user"></i>
						</span>
					</div>

					<div class="wrap-input100 validate-input m-b-10" data-validate = "Date is required">
						<input class="input100" type="date" name="StartDate" placeholder="Enter date">
						<span class="focus-input100"></span>
						<span class="symbol-input100">
							<i class="fa fa-lock"></i>
						</span>
					</div>
					<div class="container-login100-form-btn p-t-10">
						 <a href="/twitteroutput"><button class="login100-form-btn">
							Search fake posts
						</button></a>
					</div>	
				</form>
			</div>
		</div>
	</div>
	
	

	<script src="static/vendor/jquery/jquery-3.2.1.min.js"></script>
	<script src="static/vendor/designing/js/popper.js"></script>
	<script src="static/vendor/designing/js/designing.min.js"></script>
	<script src="static/vendor/select2/select2.min.js"></script>
	<script src="static/js/main.js"></script>

</body>
</html>