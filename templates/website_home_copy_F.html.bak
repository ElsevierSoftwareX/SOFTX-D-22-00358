<style>
  
  .container {
	 display: flex;
	 flex-direction: column;
	 align-items: center;
	 justify-content: center;
   padding-top: 10px;
}
 .uppercase {
	 text-transform: uppercase;
}
 .btn {
	 display: inline-block;
	 background: transparent;
	 color: inherit;
	 font: inherit;
	 border: 0;
	 outline: 0;
	 padding: 0;
	 transition: all 200ms ease-in;
	 cursor: pointer;
}
 .btn--primary {
	 background: #7f8ff4;
	 color: #fff;
	 box-shadow: 0 0 10px 2px rgba(0, 0, 0, .1);
	 border-radius: 2px;
	 padding: 12px 36px;
}
 .btn--primary:hover {
	 background: #6c7ff2;
}
 .btn--primary:active {
	 background: #7f8ff4;
	 box-shadow: inset 0 0 10px 2px rgba(0, 0, 0, .2);
}
 .btn--inside {
	 margin-left: -96px;
}
 .form__field {
	 width: 560px;
	 background: #fff;
	 color: #a3a3a3;
	 font: inherit;
   border-radius: 10px;
	 box-shadow: 0 6px 10px 0 rgba(0, 0, 0, .1);
	 border: 10;
	 outline: 0;
	 padding: 20px 18px;
}

  .buttons{
  display: flex;
  width: 500px;
  height: 300px;
 
}
.buttons .row{
  display: block;
  margin: 40px;
}
.buttons .row button{
  position: relative;
  display: block;
  height: 55px;
  width: 150px;
  color: #fff;
  font-size: 20px;
  text-shadow: 0px -1px 0px rgba(0,0,0,0.4);
  cursor: pointer;
  border: none;
  outline: none;
  border-radius: 0 5px 5px 0;
  transition: all 0s ease-out;
}
.row button:active{
  top: -3px;
}
.row button.twitter,
.row button.twitter span{
  background: #46C1F6;
  border-left: 1px solid #269BD0;
  box-shadow: 0px 5px 0px 0px #269BD0;
}
.row button.twitter:active{
  box-shadow: 0px 2px 0px 0px #269BD0;
}
.row button.reddit,
.row button.reddit span{
  background: #ff4500;
  border-left: 1px solid #DC143C;
  box-shadow: 0px 5px 0px 0px #DC143C;
}
.row button.reddit:active{
  box-shadow: 0px 2px 0px 0px #DC143C;
}
.row button span{
  position: absolute;
  height: 55px;
  width: 60px;
  top: 0;
  left: -60px;
  line-height: 55px;
  font-size: 25px;
  display: block;
  border-radius: 5px 0 0 5px;
}
.row button:active span{
  top: -3px;
}
.row button.twitter span{
  border-right: 1px solid #269BD0;
}
.row button.reddit span{
  border-right: 1px solid #DC143C;
}
.row button.twitter:active span{
  box-shadow: 0px 5px 0px 0px #269BD0,
              6px 4px 2px rgba(0,0,0,0.3);
}
.row button.reddit:active span{
  box-shadow: 0px 5px 0px 0px #DC143C,
              6px 4px 2px rgba(0,0,0,0.3);
}






* {
  margin: 0;
  padding: 0;
}

.loader {
  display: none;
  top: 50%;
  left: 50%;
  position: absolute;
  transform: translate(-50%, -50%);
}

.loading {
  border: 2px solid #ccc;
  width: 200px;
  height: 200px;
  border-radius: 50%;
  border-top-color: #1ecd97;
  border-left-color:  #1ecd97;
  animation: spin 1s infinite ease-in;
}

@keyframes spin {
  0% {
      transform: rotate(0deg);
  }
  100% {
      transform: rotate(360deg);
  }
}
</style>
